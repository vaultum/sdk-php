{
    "name": "vaultum/sdk",
    "description": "PHP SDK for Vaultum smart account - ERC-4337 account abstraction wallet",
    "type": "library",
    "keywords": [
        "vaultum",
        "smart-wallet",
        "erc4337",
        "account-abstraction",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/vaultum/vaultum",
    "license": "MIT",
    "authors": [
        {
            "name": "Vaultum Team",
            "homepage": "https://github.com/vaultum"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "VaultumSDK\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "VaultumSDK\\Test\\" : "test/" }
    }
}
